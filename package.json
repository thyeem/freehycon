{
    "name": "hycon",
    "version": "0.0.7",
    "description": "HYCON Full-node",
    "main": "index.js",
    "bin": "build/src/main.js",
    "pkg": {
        "scripts": "node_modules/stratum/lib/*.js",
        "assets": "mnemonic/*"
    },
    "scripts": {
        "stratum": "cp -f src/s_main.ts src/main.ts && ./node_modules/.bin/ts-node src/miner/freehycon.ts --port=8148 --str_port=9081 --api --api_port=2442 --cpuMiners=0 --stratum",
        "hycon": "cp -f src/c_main.ts src/main.ts && ./node_modules/.bin/ts-node src/main.ts --port=8148 --str_port=9081 --api --api_port=2442 --cpuMiners=0 --bootstrap",
        "banker": "cp -f src/c_main.ts src/main.ts && ./node_modules/.bin/ts-node src/main.ts --port=8188 --str_port=9088 --api --api_port=2442 --cpuMiners=0 --banker",
        "collector": "cp -f src/s_main.ts src/main.ts && ./node_modules/.bin/ts-node src/miner/collector.ts --collector",
        "seed": "cp -f src/c_main.ts src/main.ts && ./node_modules/.bin/ts-node src/main.ts --port=8168 --str_port=9086 --api_port=2462 --cpuMiners=0 --networkid=freehycon --bootstrap",
        "protogen": "pbjs -t static-module -w commonjs -o src/serialization/proto.js proto/main.proto &&  pbts -o src/serialization/proto.d.ts src/serialization/proto.js && ./src/serialization/trim_null.sh",
        "debug": "./node_modules/.bin/ts-node --inspect=9231 --inspect-brk src/main.ts  --port=8148 --str_port=9081 --cpuMiners=3 --api --api_port=2442 --networkid=`git rev-parse HEAD` --bootstrap",
        "debugTests": "./node_modules/.bin/ts-node  --inspect=9230 --inspect-brk  test/runner.ts",
        "test": "rm -rf build && tsc && istanbul cover node_modules/jasmine/bin/jasmine.js -x **/proto.*  -- --config=jasmine.json && remap-istanbul -i ./coverage/coverage.json -o ./coverage/remap-report -t html",
        "clear": "rm -rf blockdb* worldstate* rawblock* txDB* peerdb* minedDB*",
        "package": "tsc && pkg package.json",
        "block:build": "cd ./src/api/client && webpack"
    },
    "author": "Team HYCON",
    "license": "ISC",
